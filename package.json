  "scripts": {
    "test": "DISABLE_ESLINT_PLUGIN=true react-scripts test",
    "start": "react-scripts start",
    "startt": "snowpack dev --config snowpack.config.js",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "lint": "eslint --ext .jsx --ext .js src/ --fix",
    "hot-w": "webpack serve --hot --progress --color --mode=development",
    "wp": "webpack --progress --color --watch --mode=development",
    "fwp": "webpack --progress --color --config webpack.frontend.config.js --watch --mode=development",
    "pd": "webpack --progress --color --config webpack.config.js --mode=production && npm run i18n:php",
    "fpd": "webpack --progress --color --config webpack.frontend.config.js --mode=production",
    "pda": "npm run pd && npm run fpd",
    "clear-lint-cache": "eslint . --cache --cache-file eslint-cache/eslintcache",
    "i18n:json": "npx po2json locale.pot ../languages/bitform-bitforms-admin-script.json -f jed1.x",
    "i18n:php": "node ./pot-to-php.js locale.pot ../languages/generatedString.php bitform;wp i18n make-pot ../ --include=*.php --skip-js --slug=bitform --headers='{\"Last-Translator\":\"developer@bitcode.pro\",\"Language-Team\":\"support@bitcode.pro\",\"PO-Revision-Date\":\"\"}'",
    "stat:a": "webpack --profile --json > coverage/a.stats.json",
    "stat:f": "webpack --profile --json > coverage/f.stats.json --config webpack.frontend.config.js",
    "analyze:f": "webpack-bundle-analyzer coverage/f.stats.json",
    "clean": "rimraf node_modules yarn.lock package-lock.json pnpm-lock.yaml",
    "clean-install-run": "pnpm clean && pnpm i && pnpm hot",
    "sb": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "build-packages-pda": "cd packages/bit-country-field && yarn build && cd ../ && cd bit-file-pond && yarn build && cd ../ && cd filepond-plugin-file-validate-size && cd ../ && cd filepond-plugin-file-validate-type && yarn build && cd ../ && cd filepond-plugin-image-crop && yarn build && cd ../ && cd filepond-plugin-image-preview && yarn build && cd ../ && cd filepond-plugin-image-resize && yarn build && cd ../ && cd filepond-plugin-image-transform && yarn build && cd ../ && cd filepond-plugin-image-validate-size && yarn build && cd ../ && cd filepond-plugin-media-preview && yarn build && cd ../ && cd bit-virtualized-list && yarn build && cd ../../ && yarn pda",
    "hot": "cross-env APP_ENV=development vite"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "**/*.js?(x)": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "ie 11",
      "ie 10",
